<div id='map-container'></div>
<h1 class="title"><%= eventModel.escape('title')%></h1>
<%= eventModel.escape('location')%>
<%= eventModel.escape('date')%>
<p><%= eventModel.escape('description')%></p>
<% if (coord) {%>
  <a class='edit-form'>Edit Event</a>
<% }%><br>

<form class='search-form'>
  <label>Search for flocks within
    <select name="distance">
      <option value=5>5</option>
      <option value=15>15</option>
      <option value=25 selected>25</option>
      <option value=50>50</option>
      <option value=100>100</option>
    </select>
    miles of
    <input type='text' name='current_location'>
    <button class='search-location btn btn-sm btn-primary'>Search</button>
</form>
<hr>
<label>Coordinator
  <div class="coordinator"></div>
</label>

<div class="flock-form-container"></div>

<div role='tabpanel'>
  <ul class='nav nav-tabs' role='tablist'>
    <li role='presentation' class='active'><a href='#flocks'
      aria-controls='flocks' role='tab' data-toggle='tab'>Flocks</a></li>
    <li role='presentation'><a href='#attending' aria-controls='attending'
      role='tab' data-toggle='tab'>Attending</a></li>
  </ul>

  <div class='tab-content clearfix'>
    <div role='tabpanel' class='tab-pane active' id='flocks'>
      <div class="flocks-index"></div>
    </div>
    <div role='tabpanel' class='tab-pane' id='attending'>
      <div class="attendees-index"</div>
    </div>
  </div>
</div>
