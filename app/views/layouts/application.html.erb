<!DOCTYPE html>
<html>
<head>
  <title>flockTo</title>
  <%= stylesheet_link_tag    'application', media: 'all'%>
  <%= javascript_include_tag 'application'%>
  <%= csrf_meta_tags %>
</head>
<body>

<% if current_user%>
  <%= current_user.username %>
  <%= button_to "Sign out", session_url, method: :delete%>
<% end%>
<%= yield %>
</body>
</html>
